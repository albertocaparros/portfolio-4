---
import Header from './Header.astro';
import NavMenu from './NavMenu.astro';
import BlogHeader from './BlogHeader.astro';
import BlogNavMenu from './BlogNavMenu.astro';

const { type } = Astro.props;
---

{
	type === 'main' ? (
		<>
			<Header />
			<NavMenu />
		</>
	) : (
		<>
			<BlogHeader />
			<BlogNavMenu />
		</>
	)
}
